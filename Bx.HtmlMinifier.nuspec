<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>Bx.Sitecore-HtmlMinifier</id>
        <version>1.911.4</version>
        <title>Sitecore-HtmlMinifier</title>
        <authors>Jo David</authors>
        <owners>Jo David</owners>
        <license type="expression">MIT</license>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A cacheable Sitecore HTML Minifier pipeline</description>
		<projectUrl>https://github.com/bux/Sitecore-HtmlMinifier</projectUrl>
		<tags>Sitecore</tags>
        <dependencies>
            <group targetFramework=".NETFramework4.7.1" >
                <dependency id="Sitecore.Kernel" version="9.1.1" />
                <dependency id="Sitecore.Mvc" version="9.1.1" />
           </group>
        </dependencies>
        <contentFiles>
            <files include="config\**" buildAction="Content" />
        </contentFiles>
    </metadata>
    <files>
       <file src="config\**" target="contentFiles\App_Config\Include\zzz\" />
       <file src="config\**" target="content\App_Config\Include\zzz\" />
       <file src="src\HtmlMinifier\bin\Release\Bx.HtmlMinifier.dll" target="lib\net471" />
    </files>
</package>
