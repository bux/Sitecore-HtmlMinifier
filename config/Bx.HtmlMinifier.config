<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <mvc.renderRendering>
        <processor type="Sitecore.Mvc.Pipelines.Response.RenderRendering.ExecuteRenderer, Sitecore.Mvc">
          <patch:attribute name="type">Bx.HtmlMinifier.Pipelines.RenderRendering.ExecuteCustomRenderer, Bx.HtmlMinifier</patch:attribute>
        </processor> 
      </mvc.renderRendering>
    </pipelines>
    <services>
      <configurator type="Bx.HtmlMinifier.DependencyInjection.Configurator, Bx.HtmlMinifier"></configurator>
    </services>
  </sitecore>
</configuration>
